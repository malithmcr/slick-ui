{
  "name": "slick-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tw:watch": "npx tailwindcss -i src/tailwind.css -c tailwind.config.cjs -o src/style/index.css --watch",
    "tw:build": "NODE_ENV=production npx tailwindcss -i src/tailwind.css -c tailwind.config.cjs -o src/style/index.css --minify",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "preact": "^10.11.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@preact/preset-vite": "^2.4.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/preact": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "^8.3.0",
    "core-js": "^3.26.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
